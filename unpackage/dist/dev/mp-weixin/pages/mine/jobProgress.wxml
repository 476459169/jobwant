<view><view class="top_fixed_view"><view class="top_view"><block wx:for="{{topArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="top_view_items"><block wx:if="{{index===0}}"><view class="top_view_item"><image class="top_view_item_img" src="../../static/cv/sx_o.png" mode></image><view class="top_view_item_text_select">筛选</view></view></block><block wx:else><view data-event-opts="{{[['tap',[['topItemClick',['$0'],[[['topArr','',index]]]]]]]}}" class="top_view_item" bindtap="__e"><view class="{{[item===topSelect?'top_view_item_btn_selected':'top_view_item_btn_normal']}}"></view><view class="{{[item===topSelect?'top_view_item_text_select':'top_view_item_text_normal']}}">{{''+item+''}}</view></view></block></view></block></view></view><view class="list"><block wx:for="{{data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="cell"><view class="cell_endView"><view class="cell_endView_img"><block wx:if="{{item.status==='受邀面试'}}"><view class="cell_endView_point"></view></block></view></view><view class="cell_content"><view class="cell_top"><view class="cell_content_title">{{item.company}}</view><view class="cell_top_salary">10K~15K</view></view><view class="cell_content_msg">{{item.progress}}</view><view class="cell_content_bottomview">{{item.status}}</view></view></view><view class="line"></view></view></block></view></view>